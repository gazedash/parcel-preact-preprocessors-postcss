    "babel-preset-es2015": "^6.24.1",
    "babel-preset-preact": "^1.1.0",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.3",
    "less": "^2.7.3",
    "parcel-bundler": "^1.2.0",
    "stylus": "^0.54.5"
  },
  "dependencies": {
    "linkstate": "^1.1.0",
